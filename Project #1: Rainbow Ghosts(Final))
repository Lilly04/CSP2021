#User_Ghost Q's
c = input("What color would you like section 1 of your ghost to be?")
d = input("What color would you like section 2 of your ghost to be?")
e = input("What color would you like section 3 of your ghost to be?")
eye1 = input("What color would you like eye 1 of your ghost to be?")
eye2 = input("What color would you like eye 2 of your ghost to be?")

#User_Ghost section 1
ghost.penup()
ghost.pencolor("black")
ghost.right(90)
ghost.goto(15,55)
ghost.pendown()
ghost.fillcolor(c)
ghost.begin_fill()
ghost.left(90)
ghost.circle(20,180)
ghost.end_fill()

#User_Ghost Section 2
ghost.fillcolor(d)
ghost.begin_fill()
ghost.left(90)
ghost.forward(40)
ghost.right(90)
ghost.forward(10)
ghost.right(90)
ghost.forward(40)
ghost.right(90)
ghost.forward(10)
ghost.right(180)
ghost.forward(10)
ghost.end_fill()

#User_Ghost Section 3
ghost.fillcolor(e)
ghost.begin_fill()
ghost.forward(15)
ghost.circle(5,180)
ghost.right(180)
ghost.circle(5,180)
ghost.right(180)
ghost.circle(5,180)
ghost.right(180)
ghost.circle(5,180)
ghost.forward(15)
ghost.end_fill()

#User_Ghost Eye_1
ghost.penup()
ghost.goto(5,62.5)
ghost.pencolor(eye1)
ghost.pendown()
ghost.circle(2)

#User_Ghost Eye_2
ghost.penup()
ghost.goto(-10,62.5)
ghost.pencolor(eye2)
ghost.pendown()
ghost.circle(2)


#get painter out of screen
ghost.penup()
ghost.goto(400,0)

#end program
wn = trtl.Screen()
wn.mainloop()
