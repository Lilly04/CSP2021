# import turtle module
import turtle as trtl

#Create turtle object
ghost = trtl.Turtle()
ghost.pensize(2.5)

#Background
ghost.penup()
ghost.goto(200,190)
ghost.left(180)
ghost.pendown()
ghost.fillcolor("orange")
ghost.begin_fill()
y=0
while(y<4):
  ghost.forward(50)
  ghost.circle(50,360,3)
  ghost.forward(50)
  y=y+1
  
ghost.end_fill()
ghost.penup()
ghost.left(180)
ghost.goto(-200,-190)
ghost.pendown()
ghost.fillcolor("orange")
ghost.begin_fill()
x=0
while(x<4):
  ghost.forward(50)
  ghost.circle(50,360,3)
  ghost.forward(50)
  x=x+1
ghost.end_fill()

#Ghost_13(upper left corner) = bisexual ghost
# Ghost_13 Section 1
ghost.penup()
ghost.pencolor("black")
ghost.goto(-100,145)
ghost.pendown()
ghost.fillcolor("deep pink")
ghost.begin_fill()
ghost.left(90)
ghost.circle(20,180)
ghost.end_fill()

#Ghost_13 Section 2
ghost.fillcolor("dark magenta")
ghost.begin_fill()
ghost.left(90)
ghost.forward(40)
ghost.right(90)
ghost.forward(25)
ghost.right(90)
ghost.forward(40)
ghost.right(90)
ghost.forward(25)
ghost.right(180)
ghost.forward(25)
ghost.end_fill()

#Ghost_13 Section 3
ghost.fillcolor("navy")
ghost.begin_fill()
ghost.forward(25)
ghost.circle(5,180)
ghost.right(180)
ghost.circle(5,180)
ghost.right(180)
ghost.circle(5,180)
ghost.right(180)
ghost.circle(5,180)
ghost.forward(25)
ghost.end_fill()

#Ghost_13 Eye_1
ghost.penup()
ghost.goto(-110,150)
